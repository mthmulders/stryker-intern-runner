{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "rootDir": ".",
    "sourceMap": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "es5",
  },
  "files": [],
  "lib": [
    "dom",
    "es5",
    "es2015.promise",
    "es2015.collection",
    "es2015.iterable",
    "es2015.core",
    "es2015.symbol",
    "es2015.symbol.wellknown",
    "es2015.proxy"
  ],
  "references": [
    {
      "path": "./tsconfig.src.json"
    },
    {
      "path": "./tsconfig.test.json"
    }
  ],
  "types": [
    "node"
  ],
}